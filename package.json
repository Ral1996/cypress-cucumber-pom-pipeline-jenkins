{
  "name": "cypress-cucumber-pom-pipeline-jenkins",
  "version": "1.0.0",
  "description": "Proyecto utilizando cypress",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "runOnChromeTerminal": "cypress run --browser chrome",
    "runOnChromeBrowser": "cypress run --browser chrome --headed"
  },
  "keywords": [
    "cypress.io",
    "cucumber",
    "sonarQ",
    "pipeline",
    "pom"
  ],
  "author": "Raul Portillo, Luis Navarro, Laura Venegas",
  "license": "ISC",
  "devDependencies": {
    "crypto-js": "^4.1.1",
    "cypress": "^12.17.4",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "cypress-mochawesome-reporter": "^3.5.1"
  },
  "cypress-cucumber-preprocessor": {
    "cucumberJson": {
      "generate": true,
      "outputFolder": "cypress/cucumber-json",
      "filePrefix": "",
      "fileSuffix": ".cucumber"
    },
    "nonGlobalStepDefinitions": false,
    "stepDefinitions": "cypress/integration/PageStep/"
  }
}
